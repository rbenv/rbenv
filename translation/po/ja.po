msgstr "*rbenv global* [_＜バージョン＞_]"

#. type: Plain text
#: ../share/man/man1/rbenv.1.adoc:28
msgid "View or change the global Ruby version"
msgstr "大域的なRubyのバージョンを表示、または変更します"

#. type: Labeled list
#: ../share/man/man1/rbenv.1.adoc:29
#, no-wrap
msgid "*rbenv local* [_<version>_]"
msgstr "*rbenv local* [_＜バージョン＞_]"

#. type: Plain text
#: ../share/man/man1/rbenv.1.adoc:31
msgid "View or change the local Ruby version"
msgstr "局所的なRubyのバージョンを表示、または変更します"

#. type: Labeled list
#: ../share/man/man1/rbenv.1.adoc:32
#, no-wrap
msgid "*rbenv shell* [_<version>_]"
msgstr "*rbenv shell* [_＜バージョン＞_]"

#. type: Plain text
#: ../share/man/man1/rbenv.1.adoc:34
msgid "View or change the Ruby version for the current shell session"
msgstr "現在のシェルセッションでのRubyのバージョンを表示、または変更します"

#. type: Labeled list
#: ../share/man/man1/rbenv.1.adoc:35
#, no-wrap
